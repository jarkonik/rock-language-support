{
  "name": "rock-language-support",
  "displayName": "Rock Language Support",
  "description": "Extension adding support for Rock Language",
  "repository": {
    "type": "git",
    "url": "https://github.com/jarkonik/rock-language-support"
  },
  "version": "0.4.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "publisher": "Jaroslaw Konik",
  "icon": "icon.png",
  "contributes": {
    "languages": [
      {
        "id": "rock",
        "aliases": [
          "Rock",
          "rock"
        ],
        "extensions": [
          ".rck"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "./icon.png",
          "light": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "rock",
        "scopeName": "source.rock",
        "path": "./syntaxes/rock.tmLanguage.json"
      }
    ]
  }
}
