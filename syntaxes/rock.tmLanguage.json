{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Rock",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.rock",
          "match": "\\b(if|while|break)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.rock",
      "begin": "\"",
      "end": "\""
    }
  },
  "scopeName": "source.rock"
}
